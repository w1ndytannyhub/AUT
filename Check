_G.Login = false
	local PlayerTable = {
		["Tanny"] = "agrrjk",
		["W1ndy"] = "W1ndyxDemon",
		["smart"] = "smartxddddfg",
		["rov"] = "rovgame2009",
		["bank"] = "Bog_BigBank"
	}
	local plr = game.Players.LocalPlayer
	local X = plr.PlayerGui.TannyHub
	local Text = plr.PlayerGui.TannyHub.Frame.TextBox.Text
	if PlayerTable[Text] then
		if plr.Name == PlayerTable[Text] then
			X.Frame.Visible = false
			X.BG.Visible = true
			_G.Login = true
		elseif game.Players.LocalPlayer.Name ~= PlayerTable[Text] then
			game.Players.LocalPlayer:Kick("HWID YO YO YO YO")
		end
	end
